//c structures
#include <stdio.h>
#include <string.h>
struct student{
    char name[30];
    float height;
    char reg_no[20];
    char email[30];
    int phone;
}student1, student2;
int main() {
    // struct student1,student2;
    strcpy(student1.name,"James");
    student1.height=5.7;
    strcpy(student1.reg_no,"CT101/G/23384");
    strcpy(student1.email,"James@gmail.com");
    student1.phone=0742712001;
    printf("Name:%s\n",student1.name);
    printf("Height:%.1f\n",student1.height);
    printf("Reg_no:%s\n",student1.reg_no);
    printf("Email:%s\n",student1.email);
    printf("Phone number:%d\n",student1.phone);
    return 0;
}
